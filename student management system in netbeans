    private void b1ActionPerformed(java.awt.event.ActionEvent evt) {                                   
        try
        {
            String a = t1.getText();
            String b = t2.getText();
            String c = t3.getText();
            String d = t4.getText();
            String e = t5.getText();
            String f = t6.getText();

            Class.forName("com.mysql.jdbc.Driver");

            try (java.sql.Connection con = DriverManager.getConnection("jdbc:mysql://localhost:3306/student","root","")) {
                PreparedStatement pst=con.prepareStatement("update stud set name=?,email=?,fathersname=?,mobileno=?,address=? where studentid=?");
                pst.setString(1,b);
                pst.setString(2,c);
                pst.setString(3,d);
                pst.setString(4,e);
                pst.setString(5,f);
                pst.setString(6,a);
                int u= pst.executeUpdate();
                if(u>0)
                JOptionPane.showMessageDialog(null,"Data updated..","Msg",1);
            }
        }
        catch( SQLException ett)
        {
            JOptionPane.showMessageDialog(null,""+ett,"Msg",1);
        } catch (ClassNotFoundException ex) {
            Logger.getLogger(update.class.getName()).log(Level.SEVERE, null, ex);
        }

        // TODO add your handling code here:
    }                                  

